<div class="events row" style="width: 1085px; margin: 15px 98px 0 97px;">
  <% @moreEvents.each_slice(4) do |row| %>

 
    <div style="float: left;">
 
   <% row.each_with_index do |event, i| %>
    <a href="/events/<%= event.slug %>" />
    <div class="event-container" style="z-index: <%= (i*-1)+5%>; background-image: url(<%= event.img1(:small) %>);">
      <div class="event-overlay">
      </div>
    </div>
    

    <div class="event-inner">
            <p style="margin-left: 15px; margin-right: 15px; margin-top: 0px; font-size: 16px; color: #f200ff"><b><%= event.start_at.strftime('%d %^b') unless event.start_at.nil? %></b></p>
            <p style="margin-left: 15px; margin-right: 0; font-size: 32px; line-height: 36px;"><b><%= event.name unless event.name.nil? %></b></p>
          </div>
    </a>
    <% end %>
      </div>
  
  <% end %>
</div>

